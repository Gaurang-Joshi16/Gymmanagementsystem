<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="application.controller.StartController"
            stylesheets="@style.css"
            prefWidth="400" prefHeight="300">

    <children>
        <VBox spacing="20" alignment="CENTER" layoutX="50" layoutY="50" styleClass="root-container">
            <Label text="Welcome to Gym Management" styleClass="heading"/>
            <Button text="View Memberships" onAction="#openPlanDetails" maxWidth="200" styleClass="button"/>
            <Button text="Register Now" onAction="#openRegisterMember" maxWidth="200" styleClass="button"/>


        </VBox>
    </children>
</AnchorPane>
